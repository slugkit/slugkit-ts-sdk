{
  "name": "@slugkit/sdk",
  "version": "1.0.0",
  "description": "SlugKit SDK for JavaScript/TypeScript applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "slugkit",
    "sdk",
    "api",
    "typescript",
    "slug",
    "id-generation",
    "human-readable",
    "pattern",
    "generator"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slugkit/slugkit-ts-sdk.git"
  },
  "homepage": "https://github.com/slugkit/slugkit-ts-sdk#readme",
  "bugs": {
    "url": "https://github.com/slugkit/slugkit-ts-sdk/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "@types/node": "^20.0.0"
  },
  "peerDependencies": {},
  "dependencies": {}
}
